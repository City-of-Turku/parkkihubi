{
  "name": "ParkkiHubi",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "runserver",
  "workspaceFolder": "/parkkihub",
  "runServices": [
    "runserver",
    "postgresql",
    "memcached"
  ],
  "shutdownAction": "stopCompose",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "donjayamanne.githistory",
        "eamodio.gitlens",
        "davidanson.vscode-markdownlint",
        "dakara.transformer",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "ms-vscode.vscode-typescript-next"
      ]
    }
  },
  "postCreateCommand": "cp -n local_settings.py.tpl_dev local_settings.py"
}
